<?xml version="1.0" encoding="utf-8"?>
<pinyin xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../pinyin.cshtml">
	<char hz="冒" py="mào" multi="mò" main="1" suppressLog="1">
		<definition id="一">
			<item id="1" text="向外透，往上升：~泡. ~烟. ~火." />
			<item id="2" text="不顾（恶劣的环境或危险等）：~雨. ~险." />
			<item id="3" text="不加小心，鲁莽，冲撞：~昧. ~犯. [冒进]不顾具体条件，急于进行。 [冒失]鲁莽，轻率。" />
			<item id="4" text="用假的充当真的，假托：~牌. ~名." />
			<item id="5" text="〈古〉贪婪。" />
			<item id="6" text="姓。" />
		</definition>
	</char>
	<char hz="冒" py="mò" multi="mào">
		<phrase hz="冒顿" py="mò,dú" />
		<definition id="二">
			<item text="[冒顿](-dú)汉初匈奴的一个首领名。" />
		</definition>
	</char>
</pinyin>

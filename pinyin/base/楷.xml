<pinyin xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../pinyin.cshtml">
	<char hz="楷" py="jiē" multi="kǎi">
		<phrase hz="楷树" py="jiē,shù" />
		<definition id="二">
			<item text="楷树，落叶乔木，果实长圆形，红色。木材可制器具。也叫“黄连木”。" />
		</definition>
	</char>
	<char hz="楷" py="kǎi" multi="jiē" main="1" suppressLog="1">
		<definition id="一">
			<item id="1" text="法式，模范（(连)-模）。" />
			<item id="2" text="楷书，现在通行的一种汉字字体，是由隶书演变而来的：小~. 正~." />
		</definition>
	</char>
</pinyin>

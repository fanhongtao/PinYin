	<char hz="雀" py="qiāo" multi="qiǎo,què">
		<phrase hz="雀子" py="qiāo,zi" />
		<definition id="二">
			<item text="雀子，雀(què)斑。" />
		</definition>
	</char>
	<char hz="雀" py="qiǎo" multi="qiāo,què">
		<phrase hz="家雀儿" py="jiā,qiǎo,er" />
		<phrase hz="雀盲眼" py="qiǎo,mang,yǎn" info="〈方〉夜盲，也指夜盲人的眼睛" />
		<definition id="三">
			<item text="义同“雀(一)”，用于“家雀儿”等口语词。" />
		</definition>
	</char>
	<char hz="雀" py="què" multi="qiāo,qiǎo" main="1" suppressLog="1">
		<phrase hz="孔雀" py="kǒng,què" />
		<phrase hz="麻雀" py="má,què" />
		<phrase hz="雀斑" py="què,bān" info="皮肤病，患者多为女性。中医认为是由于火郁于孙络之血分，风邪外搏而成，生于面上，其色黄淡" />
		<phrase hz="雀盲" py="què,máng" />
		<phrase hz="雀跃" py="què,yuè" info="1.高兴地或欢闹地活动 2.如雀跳跃。比喻欣喜兴奋到极点" />
		<phrase hz="朱雀" py="zhū,què" />
		<definition id="一">
			<item text="鸟的一类，身体小，翅膀长，雌雄羽毛颜色多不相同，吃粮食粒和昆虫。特指麻雀，泛指小鸟。 [雀斑]脸上密集的褐色斑点。 [雀盲]夜盲。也说“雀盲眼”(qiǎomangyǎn)。 [雀跃](喻)高兴得像麻雀那样跳跃。" />
		</definition>
	</char>

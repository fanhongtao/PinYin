		<phrase hz="列趄" py="liè,qie" info="身躯歪斜,脚步不稳" />
		<phrase hz="趔趔趄趄" py="liè,liè,qiè,qiè" info="立脚不稳，步履踉跄。" />
		<definition id="一">
			<item text="倾斜：~着身子." />
		</definition>
	</char>
	<char hz="慊" py="qiè" multi="qiàn" main="1">
		<definition id="二">
			<item text="满足，满意：不~（不满）." />
		</definition>
	</char>
</pinyin>
<?xml version="1.0" encoding="utf-8"?>
<pinyin xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../pinyin.cshtml">
	<char hz="当" py="dāng" multi="dàng" main="1">
		<definition id="一">
			<item id="1" text="充，担任：开会~主席. 人民~了主人. (引)承担：好汉做事好汉~. [当选]选举时被选上：他~~为人民代表." />
			<item id="2" text="掌管，主持：~家. ~权. ~局." />
			<item id="3" text="介词，正在那时候或那地方：~学习的时候，不要做别的事. ~胸就是一拳. ~面说清. ~初. [当即]立刻：~~散会. [当年][当日]从前：想~~我离家的时候，这里还没有火车.（另dàng-，见“当”dàng） [当前]目前，眼下，现阶段：~~任务. [当下]马上，立刻：~~就去." />
			<item id="4" text="相当，相称(chèn)，相配：旗鼓相~. 门~户对." />
			<item id="5" text="应当，应该：~办就办. 不~问的不问." />
			<item id="6" text="顶端，头：瓜~（瓜蒂）. 瓦~（屋檐顶端的盖瓦头，俗叫“猫头”）." />
			<item id="7" text="拟声词，撞击金属器物的声音：小锣敲得~~响. [当啷](-lāng)拟声词，摇铃或其他金属器物撞击的声音：~~~~，上课铃响了." />
			<item text="[当心]留心，小心。" />
		</definition>
	</char>
	<char hz="珰" py="dāng" />
	<char hz="铛" py="dāng" multi="chēng" main="1">
		<phrase hz="锒铛" py="láng,dāng" info="锁系囚人的铁索" />
		<phrase hz="铃铛" py="líng,dang" />
		<definition id="一">
			<item text="同“当(一)(7)”。" />
		</definition>
	</char>
	<char hz="裆" py="dāng" />
	<char hz="筜" py="dāng" />
	<char hz="挡" py="dǎng" multi="dàng" main="1">
		<definition id="一">
			<item id="1" text="阻拦，遮蔽（(连)阻-、拦-）：水来土~. 把风~住. 拿芭蕉扇~着太阳." />
			<item id="2" text="（-子、-儿）用来遮蔽的东西：炉~. 窗户~儿." />
		</definition>
	</char>
	<char hz="党" py="dǎng" />
	<char hz="谠" py="dǎng" />
	<char hz="当" py="dàng" multi="dāng">
		<phrase hz="妥当" py="tuǒ,dàng" />
		<phrase hz="当作" py="dàng,zuò" />
		<phrase hz="安步当车" py="ān,bù,dàng,chē" />
		<phrase hz="当铺" py="dàng,pù" />
		<phrase hz="上当" py="shàng,dàng" />
		<definition id="二">
			<item id="1" text="恰当，合宜：处理得~. 用词不~. 妥~的办法. 适~的休息." />
			<item id="2" text="抵得上，等于：一个人~俩人用." />
			<item id="3" text="当作，作为：安步~车. 不要把他~外人. (引)认为：你~我不知道吗？" />
			<item id="4" text="表示在同一时间：他~天就走了. [当年]本年，在同一年:~~种，~~收. ~~受益.（另dāng-，见“当”dāng） [当日]本日，在同一天：~~的火车票.（另dāng-，见“当”dāng）" />
			<item id="5" text="用实物作抵押向当铺借钱。" />
			<item id="6" text="押在当铺里的实物。" />
			<item text="[上当]吃亏，受骗。" />
		</definition>
	</char>
	<char hz="垱" py="dàng" />
	<char hz="挡" py="dàng" multi="dǎng">
		<phrase hz="摒挡" py="bìng,dàng" />
		<definition id="二">
			<item text="[摒挡](bìng-)收拾，料理。" />
		</definition>
	</char>

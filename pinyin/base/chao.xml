	<char hz="绰" py="chāo" multi="chuò" info="Word主音">
		<phrase hz="绰起" py="chāo,qǐ" />
		<definition id="二">
			<item id="1" text="同“焯(二)”" />
			<item id="2" text="匆忙地抓起：~起一根棍子." />
		</definition>
	</char>
	<char hz="焯" py="chāo" multi="zhuō" info="Word主音">
		<phrase hz="焯菠菜" py="chāo,bō,cài" />
		<definition id="二">
			<item text="把蔬菜放在开水里略微一煮就拿出来：~菠菜." />
		</definition>
	</char>
	<char hz="剿" py="chāo" multi="jiǎo">
		<phrase hz="剿说" py="chāo,shuō" />
		<definition id="二">
			<item text="因袭套用别人的语言文句作为自己的：~说." />
		</definition>
	</char>
	<char hz="晁" py="cháo" />
	<char hz="巢" py="cháo" />
	<char hz="朝" py="cháo" multi="zhāo" main="1">
		<definition id="一">
			<item id="1" text="面对：坐北~南. 这个窗户~着大街. (引)介词，向着，对着：~前看. ~着宏伟的目标奋勇前进." />
			<item id="2" text="封建时代臣见君。(引)宗教徒参拜：~圣团." />
			<item id="3" text="朝廷，皇帝接见官史、发号施令的地方。[在朝](转)当政。" />
			<item id="4" text="朝代，称一姓帝王世代连续统治的时代：唐~. 改~换代." />
			<item id="5" text="姓。" />
		</definition>
	</char>
	<char hz="嘲" py="cháo" multi="zhāo" main="1">
		<definition id="一">
			<item text="讥笑，取笑（(连)-笑、-讽）：冷~热讽." />
		</definition>
	</char>
	<char hz="潮" py="cháo" />
	<char hz="吵" py="chǎo" multi="chāo" main="1">
		<definition id="一">
			<item id="1" text="声音杂乱搅扰人：~得慌. 把他~醒了." />
			<item id="2" text="打嘴架，口角：~架. 争~. 他俩~起来了." />
		</definition>
	</char>
	<char hz="炒" py="chǎo" />

	<char hz="并" py="bīng" multi="bìng">
		<phrase hz="并州" py="bīng,zhōu" />
		<definition id="二">
			<item text="并州，山西太原的别称。" />
		</definition>
	</char>
	<char hz="栟" py="bīng" multi="bēn">
		<phrase hz="栟榈" py="bīng,lǘ" />
		<definition id="二">
			<item text="[栟榈](-lǘ)古书上指棕榈。" />
		</definition>
	</char>
	<char hz="兵" py="bīng" />
	<char hz="槟" py="bīng" multi="bīn">
		<phrase hz="槟榔" py="bīng,lang" />
		<phrase hz="槟榔屿" py="bīng,láng,yǔ" info="槟城亦称“槟州”，马来西亚十三个联邦州之一" />
		<phrase hz="马槟榔" py="mǎ,bīng,láng" info="果实名。色紫味甘，可食用，亦供药用。" />
		<definition id="二">
			<item text="[槟榔](-lang)常绿乔木，生长在热带、亚热带。果实也叫槟榔，可以吃，也可入药。" />
		</definition>
	</char>
	<char hz="丙" py="bǐng" />
	<char hz="邴" py="bǐng" />
	<char hz="柄" py="bǐng" />

	<char hz="屏" py="bǐng" multi="píng">
		<phrase hz="屏除" py="bǐng,chú" />
		<phrase hz="屏弃" py="bǐng,qì" />
		<phrase hz="屏气" py="bǐng,qì" />
		<phrase hz="屏退" py="bǐng,tuì" />
		<phrase hz="屏息" py="bǐng,xī" />
		<definition id="二">
			<item id="1" text="除去，排除（(连)-除）：~弃不用. ~退左右." />
			<item id="2" text="抑止（呼吸）：~气. ~息." />
		</definition>

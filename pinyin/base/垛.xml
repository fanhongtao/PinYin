<?xml version="1.0" encoding="utf-8"?>
<pinyin xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../pinyin.cshtml">
	<char hz="垛" py="duǒ" multi="duò" main="1">
		<phrase hz="城垛" py="chéng,duǒ" info="城墙上的垛子。" />
		<phrase hz="垛子" py="duǒ,zi" info="墙两侧或上头伸出的部分。" />
		<phrase hz="箭垛" py="jiàn,duǒ" info="1.城上的短墙。亦称为「女墙」。2.射箭的标的物。或称为「箭靶子」、「箭垛子」。" />
		<phrase hz="门垛" py="mén,duǒ" info="门两旁向前方伸出的墙。" />
		<phrase hz="墙垛" py="qiáng,duǒ" info="两扇门窗或其他洞口之间的墙。" />
		<definition id="一">
			<item text="（-子）用泥土、砖石等筑成的掩蔽物：箭~子. 门~子. 城墙~口." />
		</definition>
	</char>
	<char hz="垛" py="duò" multi="duǒ">
		<phrase hz="草垛" py="cǎo,duò" />
		<phrase hz="堆垛" py="duī,duò" info="1.堆积、堆砌。2.比喻文字上的堆砌。" />
		<phrase hz="垛叠" py="duò,dié" info="堆积重叠" />
		<phrase hz="码垛" py="mǎ,duò" info="把物品整齐地堆叠好" />
		<phrase hz="麦垛" py="mài,duò" />
		<phrase hz="砖垛" py="zhuān,duò" />
		<definition id="二">
			<item id="1" text="整齐地堆成堆：麦~. 砖~." />
			<item id="2" text="整齐地堆积：柴火~得比房还高." />
		</definition>
	</char>
</pinyin>

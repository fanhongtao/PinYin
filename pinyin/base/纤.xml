	<char hz="纤" py="qiàn" multi="xiān">
		<phrase hz="拉纤" py="lā,qiàn" />
		<phrase hz="纤夫" py="qiàn,fū" />
		<!-- <phrase hz="纤手" py="qiàn,shǒu" duoyin="xiān,shǒu" info="给人介绍买卖产业的人"/> -->
		<definition id="一">
			<item text="拉船的绳。 [纤手]给人介绍买卖产业的人。 [拉纤]1.拉着纤绳使船前进。 2.给人介绍买卖产业从中取利。" />
		</definition>
	</char>

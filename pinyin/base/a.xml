	</char>
	<char hz="啊" py="ā" multi="á,ǎ,à,a" main="1">
		<phrase hz="啊哈" py="ā,hā" />
		<phrase hz="啊呀" py="ā,yā" />
		<phrase hz="啊哟" py="ā,yō" />
		<phrase hz="啊唷" py="ā,yō" />
		<definition id="一">
			<item text="叹词，表示赞叹或惊异：~，这花多好哇！~，下雪了！" />
		</definition>
	</char>
	<char hz="锕" py="ā" />

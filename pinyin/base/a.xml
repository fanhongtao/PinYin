	<char hz="呵" py="á" multi="ā,ǎ,à,a,hē">
		<definition id="二">
			<item text="同“啊(二)”" />
		</definition>
	</char>
	<char hz="啊" py="á" multi="ā,ǎ,à,a">
		<definition id="二">
			<item text="叹词，表示追问：~，你说什么？~，你再说！" />
		</definition>
	</char>
	<char hz="嗄" py="á" multi="shà">
		<definition id="一">
			<item text="同“啊(二)”" />
		</definition>
	</char>

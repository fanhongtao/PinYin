<?xml version="1.0" encoding="utf-8"?>
<pinyin xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../pinyin.cshtml">
	<char hz="纶" py="guān" multi="lún">
		<phrase hz="纶巾" py="guān,jīn" />
		<definition id="二">
			<item text="青丝带。 [纶巾]古代配有青丝带的头巾。传说三国时诸葛亮平常戴这种头巾。" />
		</definition>
	</char>
	<char hz="纶" py="lún" multi="guān" main="1" suppressLog="1">
		<phrase hz="涤纶" py="dí,lún" />
		<phrase hz="锦纶" py="jǐn,lún" />
		<phrase hz="经纶" py="jīng,lún" info="1.整理过的蚕丝。2.比喻筹划治理国家大事。3.借指抱负与才干" />
		<definition id="一">
			<item id="1" text="钓鱼用的线：垂~." />
			<item id="2" text="指某些合成的纤维：锦~. 涤~." />
		</definition>
	</char>
</pinyin>

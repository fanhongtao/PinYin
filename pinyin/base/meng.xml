	<char hz="蒙" py="mēng" multi="méng,měng">
		<phrase hz="别蒙人" py="bié,mēng,rén" />
		<phrase hz="蒙对了" py="mēng,duì,le" />
		<phrase hz="打蒙了" py="dǎ,mēng,le" />
		<definition id="二">
			<item id="1" text="欺骗（(连)-骗）：别~人. 谁也~不住他." />
			<item id="2" text="胡乱猜测：这回叫你~对了." />
			<item id="3" text="昏迷：他被球打~了." />
		</definition>
	</char>
	<char hz="氓" py="méng" multi="máng" main="1">
		<definition id="一">
			<item text="〈古〉民（特指外来的）。也作“萌”。" />
		</definition>
	</char>
	<char hz="虻" py="méng" />
	<char hz="萌" py="méng">
		<definition>
			<item id="1" text="植物的芽。" />
			<item id="2" text="萌芽，植物生芽。 (引)开始发生：知者（有见识的人）见于未~. 故态复~（多用于贬义）." />
		</definition>
	</char>
	<char hz="盟" py="méng" />
	<char hz="蒙" py="méng" multi="mēng,měng" main="1">
		<definition id="一">
			<item id="1" text="没有知识，愚昧：启~. 发~. ~昧." />
			<item id="2" text="覆盖：~头盖脑. ~上一张纸. [蒙蔽]隐瞒事实，欺骗。" />
			<item id="3" text="受：~难(nàn). 承~招待，感谢之至." />
			<item id="4" text="形容雨点细小（叠）：~~细雨. [空蒙]形容景色迷茫：山色~~." />
			<item id="5" text="眼睛失明。 [蒙眬]目不明：睡眼~~." />
			<item id="6" text="朴实敦厚。" />
			<item id="7" text="姓。" />
		</definition>
	</char>
	<char hz="濛" py="méng" info="词组中使用到，所以补充">
		<definition>
			<item text="“蒙(一)(4)”的繁体字。"/>
		</definition>
	</char>

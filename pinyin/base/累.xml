	<char hz="累" py="léi" multi="lěi,lèi">
		<phrase hz="果实累累" py="guǒ,shí,léi,léi" />
		<phrase hz="累赘" py="léi,zhui" />
		<definition id="三">
			<item id="1" text="连缀或捆。 [累累]连续成串：果实~~. [累赘](-zhui)多余的负担，麻烦：这事多~~." />
			<item id="2" text="绳索。" />
			<item id="3" text="缠绕。" />
		</definition>

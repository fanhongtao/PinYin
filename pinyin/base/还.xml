<?xml version="1.0" encoding="utf-8"?>
<pinyin xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../pinyin.cshtml">
	<char hz="还" py="hái" multi="huán" main="1">
		<phrase hz="还好" py="hái,hǎo" info="1.过得去;不坏。2.幸运地。" />
		<phrase hz="还是" py="hái,shì" />
		<phrase hz="还有" py="hái,yǒu" />
		<definition id="二">
			<item id="1" text="仍旧，依然：你~是那样. 这件事~没有做完." />
			<item id="2" text="更：今天比昨天~热." />
			<item id="3" text="再，又：另外~有一件事要做. 提高产量，~要保证质量." />
			<item id="4" text="尚，勉强过得去：身体~好. 工作进展得~不算慢." />
			<item id="5" text="尚且：他那么大年经~这么干，咱们更应该加油干了. [还是](-shi)1.副词，表示这么办比较好：咱们~~出去吧. 2.连词，用在问句里表示选择：是你去呢，~~他来？" />
			<item id="6" text="表示对某件事物，没想到如此，而居然如此：他~真有办法." />
		</definition>
	</char>
	<char hz="还" py="huán" multi="hái">
		<phrase hz="偿还" py="cháng,huán" />
		<phrase hz="发还" py="fā,huán" info="把收来的东西还回去" />
		<phrase hz="返老还童" py="fǎn,lǎo,huán,tóng" />
		<phrase hz="奉还" py="fèng,huán" info="敬辞。送还。" />
		<phrase hz="归还" py="guī,huán" />
		<phrase hz="还本" py="huán,běn" info="还付所借的本钱" />
		<phrase hz="还击" py="huán,jī" info="回击" />
		<phrase hz="还家" py="huán,jiā" />
		<phrase hz="还口" py="huán,kǒu" info="回嘴" />
		<phrase hz="还礼" py="huán,lǐ" />
		<phrase hz="还钱" py="huán,qián" />
		<phrase hz="还手" py="huán,shǒu" />
		<phrase hz="还书" py="huán,shū" />
		<phrase hz="还俗" py="huán,sú" info="出家的僧、尼、道恢复普通人的身分" />
		<phrase hz="还乡" py="huán,xiāng" info="1.返回乡里。2.反转方向。乡，通“向”、“嚮”。" />
		<phrase hz="还原" py="huán,yuán" />
		<phrase hz="还账" py="huán,zhàng" info="归还所欠的债或偿付所欠的货款" />
		<phrase hz="回还" py="huí,huán" info="返回原处" />
		<phrase hz="往还" py="wǎng,huán" info="去和来;往来" />
		<phrase hz="以牙还牙" py="yǐ,yá,huán,yá" />
		<phrase hz="以眼还眼" py="yǐ,yǎn,huán,yǎn" />
		<definition id="一">
			<item id="1" text="回，归：~家. ~原（恢复原状）." />
			<item id="2" text="回报：~礼. ~手. 以眼~眼，以牙~牙." />
			<item id="3" text="归还（(连)偿-）：~钱." />
			<item id="4" text="姓。" />
		</definition>
	</char>
</pinyin>

		<phrase hz="哄劝" py="hǒng,quàn" info="用言辞劝说，使人乐意接受。" />
		<phrase hz="哄人" py="hǒng,rén" />
		<phrase hz="哄小孩" py="hǒng,xiǎo,hái" />
		<phrase hz="瞒哄" py="mán,hǒng" info="隐瞒欺骗。" />
		<phrase hz="蒙哄" py="méng,hǒng" info="指使…信假为真。" />
		<phrase hz="骗哄" py="piàn,hǒng" info="哄骗。" />
		<phrase hz="欺哄" py="qī,hǒng" info="欺骗；哄骗。" />
		<phrase hz="诈哄" py="zhà,hǒng" info="哄骗。" />
		<definition id="二">
			<item id="1" text="说假话骗人（(连)-骗）：你不要~我." />
			<item id="2" text="用语言或行动使人欢喜：他很会~小孩儿." />
		</definition>

	<char hz="石" py="dàn" multi="shí">
		<definition id="二">
			<item text="市制容量单位，1石是10斗，合100升。（此义在古书中读shí，如“二千石”）" />
		</definition>
	</char>
	<char hz="旦" py="dàn" />
	<char hz="但" py="dàn" />
	<char hz="担" py="dàn" multi="dān,dǎn">
		<phrase hz="扁担" py="biǎn,dàn" />
		<phrase hz="担子" py="dàn,zi" />
		<phrase hz="货郎担" py="huò,láng,dàn" />
		<phrase hz="重担" py="zhòng,dàn" />
		<definition id="二">
			<item id="1" text="扁担，挑东西的用具，多用竹、木做成。" />
			<item id="2" text="（-子）一挑儿东西：货郎~. (喻)担负的责任：重~. 不怕~子重." />
			<item id="3" text="市制重量单位，1担是100斤，合50千克。" />
			<item id="4" text="量词：一~水。" />
		</definition>

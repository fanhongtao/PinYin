	<char hz="扇" py="shān" multi="shàn">
		<phrase hz="用扇子扇" py="yòng,shàn,zi,shān" />
		<definition id="二">
			<item id="1" text="摇动扇子或其他东西，使空气加速流动生风：用扇子~." />
			<item id="2" text="用手掌打：~了他一耳光." />
			<item id="3" text="同“煽(2)”。" />
		</definition>
	</char>

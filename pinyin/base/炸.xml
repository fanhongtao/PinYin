<?xml version="1.0" encoding="utf-8"?>
<pinyin xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../pinyin.cshtml">
	<char hz="炸" py="zhá" multi="zhà">
		<phrase hz="油炸" py="yóu,zhá" />
		<phrase hz="炸糕" py="zhá,gāo" />
		<phrase hz="炸鱼" py="zhá,yú" />
		<definition id="二">
			<item text="把食物放在煮沸的油或水里弄熟：~糕. ~鱼. 把菠菜~一~." />
		</definition>
	</char>
	<char hz="炸" py="zhà" multi="zhá" main="1">
		<phrase hz="爆炸" py="bào,zhà" info="物体体积急剧膨大炸裂，使周围气压发生强烈变化并产生巨大声响的现象" />
		<phrase hz="轰炸" py="hōng,zhà" info="用炸弹袭击;往投弹;向投弹" />
		<phrase hz="炸弹" py="zhà,dàn" info="一种爆炸武器，通常是铁壳内装入炸药而成。 一般由飞机投掷" />
		<phrase hz="炸锅" py="zhà,guō" info="锅里的食物向锅外四散迸溅。 比喻人群中发生吵闹动乱" />
		<phrase hz="炸呼" py="zhà,hū" info="乱喊叫；吆喝。" />
		<phrase hz="炸毁" py="zhà,huǐ" info="用爆破手段或用炸弹等摧毁" />
		<phrase hz="炸雷" py="zhà,léi" info="指声音非常响的雷。亦比喻很响的声音" />
		<phrase hz="炸裂" py="zhà,liè" info="以爆破法使破裂" />
		<phrase hz="炸毛" py="zhà,máo" info="〈方〉发脾气；发怒。" />
		<phrase hz="炸破" py="zhà,pò" info="因内部的压力而分裂成碎片" />
		<phrase hz="炸群" py="zhà,qún" info="指动物受惊各自离群逃散。" />
		<phrase hz="炸市" py="zhà,shì" info="闹市上人们因受惊扰而四处乱跑。" />
		<phrase hz="炸碎" py="zhà,suì" info="用炸药使被炸物体(如岩石)碎裂" />
		<phrase hz="炸窝" py="zhà,wō" info="〈方〉 1.成群的蜂、鸟从窝中惊乱飞散 2.一群人因惊吓乱成一窝蜂" />
		<phrase hz="炸响" py="zhà,xiǎng" info="形容雷声、鞭声等响亮。" />
		<phrase hz="炸牙" py="zhà,yá" info="形容冷得刺牙。" />
		<phrase hz="炸眼" py="zhà,yǎn" info="马、驴等牲畜突然见到生人或其他不熟悉的东西而惊恐、嘶鸣或狂奔。" />
		<phrase hz="炸药" py="zhà,yào" info="在一定的外界能量作用下，能由其本身的能量发生爆炸的物质。 炸药爆炸时，能释放出大量的热能并产生高温高压气体，对周围物质起破坏、抛掷、压缩等作用" />
		<phrase hz="炸营" py="zhà,yíng" info="兵营发生骚乱，比喻人群骚动，乱成一团" />
		<definition id="一">
			<item id="1" text="突然破裂（(连)爆-）：~弹. 玻璃杯~了." />
			<item id="2" text="用炸药、炸弹爆破：~碉堡." />
			<item id="3" text="发怒：他一听就~了." />
			<item id="4" text="因受惊而四处逃散：~市. ~窝." />
		</definition>
	</char>
</pinyin>
